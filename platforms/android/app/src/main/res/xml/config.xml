<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yuhupos.id" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Thermalprint">
        <param name="android-package" value="com.giorgiofellipe.datecsprinter.DatecsPrinter" />
    </feature>
    <feature name="BluetoothSerial">
        <param name="android-package" value="com.megster.cordova.BluetoothSerial" />
    </feature>
    <name>Yuhu POS</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-networking-bluetooth" source="npm" spec="~1.0.3" />
    <gap:plugin name="cordova-plugin-bluetooth-zbtprinter" source="npm" spec="~1.0" />
    <gap:plugin name="cordova-plugin-thermalprint" source="npm" spec="~1.3.8" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-targetSdkVersion" value="27" />
    <preference name="android-minSdkVersion" value="27" />
</widget>
